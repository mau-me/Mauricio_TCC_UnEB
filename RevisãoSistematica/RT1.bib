@INPROCEEDINGS{abstractLearningStackQueue_Park_2017,
author={Park, Byron and Ahmed, Dewan Tanvir},
booktitle={2017 International Conference on Computational Science and Computational Intelligence (CSCI)}, 
title={Abstracting Learning Methods for Stack and Queue Data Structures in Video Games},
year={2017},
volume={},
number={},
pages={1051-1054},
abstract={Computer Science students will often mention the steep learning curve when first starting to learn computing topics. It is apparent, albeit, necessary, and consequentially results in a high attrition rate of students pursuing a Computer Science related education. Learning computing concepts, particularly for students who have had little to no experience prior, can be very difficult. The workload is heavy and often very tedious, which leads to a corresponding lack of motivation to learn. Beyond the initial difficulties, students must take numbers of courses centered mostly on complex, conceptual topics. Courses that teach data structures and algorithms are required in most schools early on in a Computer Science student's college career, and much like the introduction courses, these can be tedious and difficult to grasp. Our goal is to provide an interesting, effective, and measurable tool for learning these concepts, which we have developed in the form of a video game. This paper is focused on the implementation of stack and queue data structures within our game. A random sample of students have play-tested our game and provided comments on their experience with the playthrough. The feedback we received provides some important insights on the teaching and learning methods used in the classroom.},
keywords={gamification;serious games;CS education;engagement;motivation},
doi={10.1109/CSCI.2017.183},
ISSN={},
month={Dec},
}

